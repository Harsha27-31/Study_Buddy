      // PDF viewer controls
      elements.pdfViewer.closeBtn.addEventListener('click', pdfViewer.close);
      elements.pdfViewer.prevPage.addEventListener('click', () => {
        if (state.pdf.pageNum <= 1) return;
        state.pdf.pageNum--;
