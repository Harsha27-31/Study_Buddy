      elements.pdfViewer.nextPage.addEventListener('click', () => {
        if (state.pdf.pageNum >= state.pdf.doc?.numPages) return;
        state.pdf.pageNum++;
        pdfViewer.queueRenderPage(state.pdf.pageNum);
      });
      elements.pdfViewer.downloadBtn.addEventListener('click', pdfViewer.download);

