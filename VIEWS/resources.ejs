      render: (resources) => {
        elements.resourcesContainer.innerHTML = "";

        if (resources.length === 0) {
          elements.resourcesContainer.innerHTML = `
            <div class="empty-state">
              <i class="fas fa-search"></i>
              <p>No resources match your selected filters</p>
              <p>Try adjusting your filters</p>
