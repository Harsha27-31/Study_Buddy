        });

        document.querySelectorAll('.download-pdf').forEach(btn => {
          btn.addEventListener('click', () => {
            const url = btn.getAttribute('data-url');
            const title = btn.getAttribute('data-title');
            resourceManager.downloadPdf(url, title);
          });
        });
      },
      
      downloadPdf: (url, title) => {
        const fileId = utils.extractGoogleDriveFileId(url);
        const downloadUrl = fileId 
          ? `https://drive.google.com/uc?export=download&id=${fileId}`
          : url;
          
        const a = document.createElement('a');
        a.href = downloadUrl;
        a.download = utils.sanitizeFilename(title);
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
      }
    };

    // Event Listeners
    const setupEventListeners = () => {
      // Filter changes
      Object.values(elements.filters).forEach(filter => {
        filter.addEventListener("change", resourceManager.filter);
