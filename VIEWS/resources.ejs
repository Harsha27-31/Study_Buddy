    // Utility Functions
    const utils = {
      extractGoogleDriveFileId: (url) => {
        // Handles multiple Google Drive URL formats
        const patterns = [
          /\/file\/d\/([^\/]+)/,
          /[?&]id=([^&]+)/,
          /\/uc\?id=([^&]+)/,
          /\/open\?id=([^&]+)/
        ];
        
        for (const pattern of patterns) {
          const matches = url.match(pattern);
          if (matches && matches[1]) {
            return matches[1];
          }
        }
        return null;
      },
      
      getDirectPdfUrl: (url) => {
        const fileId = utils.extractGoogleDriveFileId(url);
        if (fileId) {
          // This is the correct format for direct PDF access
          return `https://drive.google.com/uc?export=view&id=${fileId}`;
        }
        return url;
      },
      
      sanitizeFilename: (name) => {
        return name.replace(/[^a-z0-9]/gi, '_') + '.pdf';
      },
      
      isGoogleDriveUrl: (url) => {
        return url.includes('drive.google.com');
      }
    };

    // PDF Viewer Functions
    const pdfViewer = {
      renderPage: (num) => {
        state.pdf.rendering = true;
        
        state.pdf.doc.getPage(num).then((page) => {
          const viewport = page.getViewport({ scale: state.pdf.scale });
          elements.pdfViewer.canvas.height = viewport.height;
          elements.pdfViewer.canvas.width = viewport.width;

          const renderContext = {
            canvasContext: elements.pdfViewer.canvas.getContext('2d'),
            viewport: viewport
          };

          const renderTask = page.render(renderContext);
          
          renderTask.promise.then(() => {
            state.pdf.rendering = false;
            elements.pdfViewer.currentPage.textContent = num;
            
            if (state.pdf.pendingPageNum !== null) {
              pdfViewer.renderPage(state.pdf.pendingPageNum);
              state.pdf.pendingPageNum = null;
            }
          });
        }).catch(error => {
          console.error("Error rendering page:", error);
          pdfViewer.showError("Failed to render PDF page");
        });
      },
      
      queueRenderPage: (num) => {
        if (state.pdf.rendering) {
          state.pdf.pendingPageNum = num;
        } else {
          pdfViewer.renderPage(num);
        }
      },
      
      open: (title, url) => {
        state.pdf.currentTitle = title;
        
        // Handle Google Drive links specially
        if (utils.isGoogleDriveUrl(url)) {
          const fileId = utils.extractGoogleDriveFileId(url);
          if (fileId) {
            // Option 1: Open in Google's viewer (recommended)
            const previewUrl = `https://drive.google.com/file/d/${fileId}/preview`;
            const usePdfJsFallback = confirm(
          "Open this PDF in Google Drive viewer?\n\n" +
          "Click OK to open in Google Drive (recommended).\n" +
          "Click Cancel to try our built-in viewer instead."
        );
        
        if (usePdfJsFallback) {
          window.open(previewUrl, '_blank');
        } else {
          const directUrl = `https://drive.google.com/uc?export=view&id=${fileId}`;
          pdfViewer.showInModal(title, directUrl);
        }
        return;
      }
    }
        
        // For non-Google Drive PDFs
        pdfViewer.showInModal(title, utils.getDirectPdfUrl(url));
      },
      
      showInModal: (title, url) => {
        elements.pdfViewer.title.textContent = title;
        state.pdf.currentUrl = url;
        elements.pdfViewer.modal.classList.add('active');
        
        // Clear previous PDF
        if (state.pdf.doc) {
          state.pdf.doc.destroy();
          state.pdf.doc = null;
        }
        
        // Load the PDF with CORS headers
        const loadingTask = pdfjsLib.getDocument({
          url: url,
          withCredentials: false,
          httpHeaders: {
            'Access-Control-Allow-Origin': '*'
          }
        });
        
        loadingTask.promise.then((pdf) => {
          state.pdf.doc = pdf;
          elements.pdfViewer.totalPages.textContent = pdf.numPages;
          state.pdf.pageNum = 1;
          pdfViewer.renderPage(state.pdf.pageNum);
        }).catch((error) => {
          console.error('PDF loading error:', error);
          let errorMessage = "Failed to load PDF. Please try downloading the file instead.";
          
          if (error.name === 'InvalidPDFException') {
            errorMessage = "The file is not a valid PDF";
          } else if (error.message.includes('CORS')) {
            errorMessage = "CORS error - try downloading instead";
          }
          
          pdfViewer.showError(errorMessage);
        });
      },
      
      showError: (message) => {
        const userResponse = confirm(`${message}\n\nWould you like to:\n1. Try downloading instead?\n2. Open in Google Drive?`);
        
        if (userResponse) {
          // Try downloading
          pdfViewer.download();
        } else {
          // Open in Google Drive
          if (state.pdf.currentUrl && utils.isGoogleDriveUrl(state.pdf.currentUrl)) {
            const fileId = utils.extractGoogleDriveFileId(state.pdf.currentUrl);
            if (fileId) {
              window.open(`https://drive.google.com/file/d/${fileId}/view`, '_blank');
            }
          }
        }
        
        pdfViewer.close();
      },
      
      close: () => {
        elements.pdfViewer.modal.classList.remove('active');
        if (state.pdf.doc) {
          state.pdf.doc.destroy();
          state.pdf.doc = null;
        }
      },
      
      download: () => {
        if (!state.pdf.currentUrl) return;
        
        const downloadUrl = utils.isGoogleDriveUrl(state.pdf.currentUrl) 
          ? `https://drive.google.com/uc?export=download&id=${utils.extractGoogleDriveFileId(state.pdf.currentUrl)}`
          : state.pdf.currentUrl;
          
        const a = document.createElement('a');
        a.href = downloadUrl;
        a.download = utils.sanitizeFilename(state.pdf.currentTitle);
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        
        // Fallback for mobile devices
        setTimeout(() => {
          if (!document.body.contains(a)) {
            window.open(downloadUrl, '_blank');
          }
        }, 200);
      }
    };

    // Resource Management
    const resourceManager = {
      filter: () => {
        const filters = {
          year: elements.filters.year.value,
          sem: elements.filters.sem.value,
          subject: elements.filters.subject.value,
          unit: elements.filters.unit.value
        };

        const filtered = state.resources.filter(item =>
          (!filters.year || item.year === filters.year) &&
          (!filters.sem || item.semester === filters.sem) &&
          (!filters.subject || item.subject === filters.subject) &&
          (!filters.unit || item.unit === filters.unit)
        );

        resourceManager.render(filtered);
