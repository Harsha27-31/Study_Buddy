      window.addEventListener('resize', () => {
        if (elements.pdfViewer.modal.classList.contains('active') && state.pdf.doc) {
          pdfViewer.queueRenderPage(state.pdf.pageNum);
        }
      });
    };

    // Initialize Application
    const init = () => {
      setupEventListeners();
      resourceManager.filter(); // Initial render
    };

    // Start the app
    init();
  </script>
</body>
