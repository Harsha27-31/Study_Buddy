              </button>
              <button class="btn btn-outline download-pdf" 
                      data-url="${item.link}" 
                      data-title="${item.topic}">
                <i class="fas fa-download"></i> Download
              </button>
            </div>
          `;

          elements.resourcesContainer.appendChild(card);
